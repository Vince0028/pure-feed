# YouTube Data API v3 — Get yours at https://console.cloud.google.com
YOUTUBE_API_KEY=your_youtube_api_key_here

# Google Gemini — Get yours at https://aistudio.google.com
# Single key (backwards compatible):
GEMINI_API_KEY=your_gemini_api_key_here
# Multiple keys for auto-rotation (comma-separated) — when one runs out, swaps to the next:
GEMINI_API_KEYS=key1_here,key2_here,key3_here

# Vercel Cron Secret — set in Vercel dashboard
CRON_SECRET=your_cron_secret_here

# Server
PORT=3001
FRONTEND_URL=http://localhost:8080
